@model Księgarnia.Models.Delivery

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Delivery</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            
            <div class="form-group">
                <label asp-for="ParcelLockerId" class="control-label"></label>
                <select asp-for="ParcelLockerId" class ="form-control" asp-items="ViewBag.ParcelLockerId"></select>
            </div>
            
            <div class="form-group">
                <input type="submit" value="Dalej" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Dalej</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
